algoritmo "semnome"
// Função :
// Autor :
// Data : 13/10/2023
// Seção de Declarações 
var
   numero:inteiro
   impar: inteiro
inicio
      escreva("o numero é: ")
      leia (impar)
      numero<-1

      ENQUANTO (numero <= impar) FACA
      escreva(numero)
      numero<- numero+ 2
      FIMENQUANTO
fimalgoritmo


algoritmo "semnome"
// Função :
// Autor :
// Data : 06/09/2023
// Seção de Declarações 
var
   ano:inteiro
   idade: inteiro
inicio
     //ano <- "2000"      // var ano recebe valor 2000
      leia(ano)         // var ano recebe valor digitado
      escreval("Que ano vc nasceu: ", ano) // envio da var ano
      idade <- 2023 - ano

      escreva ("Quantos anos vc tem:", idade)
fimalgoritmo

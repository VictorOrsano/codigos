algoritmo "semnome"
// Função :
// Autor :
// Data : 23/10/2023
// Seção de Declarações 
var
   contador: inteiro
inicio
      contador <- 100
      ENQUANTO (contador >= 0) FACA
           escreva(contador)
           contador<-contador-5
           SE (contador>= 0) ENTAO
           escreva(",")
           fimse
      FIMENQUANTO
      escreva("terminou.")
fimalgoritmo


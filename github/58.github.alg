algoritmo "semnome"
// Função :
// Autor :
// Data : 07/11/2023
// Seção de Declarações 
var
   idade, alunos: inteiro
   media:real
inicio
      escreva("Qual seria a idade: ")
      leia(idade)
      
      
      ENQUANTO ( idade <> 999 ) FACA

               


      fimenquanto
      media<-idade/alunos
      
      
fimalgoritmo

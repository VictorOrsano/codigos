algoritmo "semnome"
// Função :
// Autor :
// Data : 23/10/2023
// Seção de Declarações 
var
   contador: inteiro
   numero : inteiro
inicio
      escreval("qual o numero inteiro? " )
      leia(numero)
      
      contador <- numero
      ENQUANTO (contador <= 35) FACA
           escreva(contador)
           contador<-contador+1
      FIMENQUANTO
      escreva("terminou.")
fimalgoritmo

